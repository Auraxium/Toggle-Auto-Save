{
  "name": "smart-save",
  "displayName": "Toggle Auto Save",
  "description": "Exclude certain files from auto saving",
  "version": "1.0.5",
  "publisher": "aura-pass",
  "repository": {
    "type": "git",
    "url": "https://github.com/Auraxium/Toggle-Auto-Save.git"
},
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartup", "onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "smart-save.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}